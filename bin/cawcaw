#!/usr/bin/env ruby

require "cawcaw"

command = ARGV.shift

exit_code = 0

case command
when "hadoop"
  exit_code = Cawcaw::Command::Hadoop.run(ARGV) || 0
when "rabbitmq"
  exit_code = Cawcaw::Command::Rabbitmq.run(ARGV) || 0
else
  STDERR.puts "invalid command: '#{command}'"
  exit_code = 1
end

exit exit_code